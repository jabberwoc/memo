{
  "name": "memo",
  "version": "0.6.1",
  "description": "an app for taking notes",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "electron": "electron",
    "electron-rebuild": "electron-rebuild",
    "electron-packager": "electron-packager",
    "electron-installer-debian": "electron-installer-debian",
    "electron-installer-windows": "electron-installer-windows",
    "electron-installer-dmg": "electron-installer-dmg",
    "clean": "rm -rf build",
    "build": "npm run ng -- build --output-path=build --base-href=./",
    "build-prod": "npm run ng -- build --prod --output-path=build --base-href=./",
    "rebuild-modules": "npm run electron-rebuild -- --module-dir build",
    "electron-install": "cp -aL electron/. build && cd build && npm install --production && cd .. && npm run rebuild-modules",
    "build:electron": "npm run clean && npm run build-prod && npm run electron-install",
    "dist:linux": "npm run electron-packager -- build --platform=linux --arch=x64 --prune=true --out=dist/ --asar --overwrite",
    "dist:win": "npm run electron-packager -- build --platform=win32 --arch=x64 --prune=true --icon=assets/icons/win/puffin.ico --out=dist/ --asar --overwrite",
    "dist:win32": "npm run electron-packager -- build --platform=win32 --arch=ia32 --prune=true --icon=assets/icons/win/puffin.ico --out=dist/ --asar --overwrite",
    "dist:mac": "npm run electron-packager -- build --platform=darwin --arch=x64 --prune=true --icon=assets/icons/mac/puffin.icns --out=dist/ --asar --overwrite",
    "package:debian": "npm run electron-installer-debian -- --arch amd64 --config deb.json",
    "package:mac": "npm run electron-installer-dmg -- dist/memo-darwin-x64/memo.app memo",
    "package:win": "npm run electron-installer-windows -- --src dist/memo-win32-x64/ --dest dist/installers/",
    "test": "npm run ng test",
    "lint": "npm run ng lint",
    "e2e": "npm run ng e2e",
    "serve": "npm run ng -- serve",
    "start-electron": "npm run electron -- . --remote-debugging-port=9222",
    "start": "npm run build && npm run start-electron",
    "dev": "npm run start-electron | npm run serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/platform-server": "7.1.1",
    "@angular/router": "7.1.1",
    "@ngrx/store": "^6.1.2",
    "@types/cuid": "^1.3.0",
    "@types/lodash": "^4.14.119",
    "@types/pouchdb": "^6.3.2",
    "ajv": "^6.6.2",
    "angular-notifier": "^4.1.1",
    "core-js": "^2.6.2",
    "cuid": "^2.1.4",
    "docuri": "^4.2.2",
    "dotenv": "^6.2.0",
    "electron-reload": "^1.4.0",
    "electron-settings": "^3.2.0",
    "file-saver": "^2.0.0",
    "font-awesome": "^4.7.0",
    "keytar": "^4.3.0",
    "lodash": "^4.17.11",
    "ngx-fs": "0.0.1",
    "ngx-logger": "^3.3.9",
    "ngx-moment": "^3.3.0",
    "path": "^0.12.7",
    "photonkit": "^0.1.2",
    "pouchdb": "^6.4.3",
    "pouchdb-all-dbs": "^1.0.2",
    "pouchdb-authentication": "^1.1.3",
    "rxjs": "^6.3.3",
    "split.js": "^1.5.10",
    "tinymce": "^4.9.2",
    "zone.js": "^0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "7.1.0",
    "@angular/compiler-cli": "7.1.1",
    "@angular/language-service": "7.1.1",
    "@types/file-saver": "^2.0.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "codelyzer": "~4.5.0",
    "electron": "^3.0.10",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-windows": "^1.1.0",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2",
    "jasmine": "^3.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "moment": "^2.23.0",
    "ngx-electron": "^2.1.1",
    "node-abi": "^2.5.1",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "3.1.6"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^1.1.0"
  }
}
