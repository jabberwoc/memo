{
  "name": "memo",
  "version": "0.9.1",
  "description": "an app for taking notes",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "ng": "ng",
    "electron": "electron",
    "electron-packager": "electron-packager",
    "electron-installer-debian": "electron-installer-debian",
    "electron-installer-windows": "electron-installer-windows",
    "electron-installer-dmg": "electron-installer-dmg",
    "clean": "rm -rf build",
    "build": "npm run ng -- build --output-path=build --base-href=./",
    "build-prod": "npm run ng -- build --configuration production --output-path=build --base-href=./",
    "electron-install": "cp -aL electron/. build && cd build && npm install --production",
    "build:electron": "npm run clean && npm run build-prod && npm run electron-install",
    "dist:linux": "npm run electron-packager -- build --platform=linux --arch=x64 --prune=true --out=dist/ --asar --overwrite",
    "dist:win": "npm run electron-packager -- build --platform=win32 --arch=x64 --prune=true --icon=assets/icons/win/puffin.ico --out=dist/ --asar --overwrite",
    "dist:win32": "npm run electron-packager -- build --platform=win32 --arch=ia32 --prune=true --icon=assets/icons/win/puffin.ico --out=dist/ --asar --overwrite",
    "dist:mac": "npm run electron-packager -- build --platform=darwin --arch=x64 --prune=true --icon=assets/icons/mac/puffin.icns --out=dist/ --asar --overwrite",
    "package:debian": "npm run electron-installer-debian -- --arch amd64 --config deb.json",
    "package:mac": "npm run electron-installer-dmg -- dist/memo-darwin-x64/memo.app memo",
    "package:win": "npm run electron-installer-windows -- --src dist/memo-win32-x64/ --dest dist/installers/",
    "test": "npm run ng test",
    "lint": "npm run ng lint",
    "e2e": "npm run ng e2e",
    "serve": "npm run ng -- serve",
    "start-electron": "npm run electron -- . --remote-debugging-port=9222",
    "start": "npm run build && npm run start-electron",
    "dev": "npm run start-electron | npm run serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/platform-server": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@electron/remote": "^2.0.9",
    "@ngrx/store": "^15.4.0",
    "@types/cuid": "^1.3.1",
    "@types/lodash": "^4.14.179",
    "@types/pouchdb": "^6.4.0",
    "ajv": "^8.10.0",
    "angular-notifier": "^13.0.0",
    "core-js": "^3.21.1",
    "cuid": "^2.1.8",
    "docuri": "^4.2.2",
    "dotenv": "^16.0.0",
    "electron-reload": "^1.5.0",
    "electron-settings": "^4.0.2",
    "electron-store": "^8.1.0",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "nano": "^10.1.2",
    "ngx-logger": "^5.0.9",
    "ngx-moment": "^6.0.2",
    "path": "^0.12.7",
    "photonkit": "^0.1.2",
    "pouchdb-browser": "^8.0.1",
    "rxjs": "^7.5.4",
    "split.js": "^1.6.5",
    "tinymce": "^4.9.3",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^14.18.42",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "electron": "^27.0.2",
    "electron-installer-dmg": "^4.0.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "17.1.2",
    "eslint": "^8.33.0",
    "jasmine": "^4.0.2",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "moment": "^2.29.1",
    "node-abi": "^3.8.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.0",
    "typescript": "4.9.5"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^3.2.0"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}